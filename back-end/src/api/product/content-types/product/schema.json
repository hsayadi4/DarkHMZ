{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "blocks",
      "required": true
    },
    "Image": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "Price": {
      "type": "decimal",
      "required": true
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Homme",
        "Femme"
      ],
      "required": true
    }
  }
}
